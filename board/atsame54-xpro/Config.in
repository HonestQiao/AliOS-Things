config AOS_BOARD_ATSAME54_XPRO
    bool "board_atsame54-xpro"
    select AOS_MCU_ATSAME54
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_SAL if AOS_NETWORK_SAL
    select AOS_COMP_NETMGR if AOS_NETWORK_SAL
    help
      configuration for board atsame54-xpro

if AOS_BOARD_ATSAME54_XPRO
# Configurations for board board_atsame54-xpro

# Set default value of AOS_NETWORK_SAL
config AOS_NETWORK_SAL
    bool "AOS_NETWORK_SAL"
    default y

config SAL_MODULE
    string
    default "wifi.mk3060"

menu "BSP SUPPORT FEATURE"

config BSP_SUPPORT_UART
    bool "BSP_SUPPORT_UART"
    default y

config BSP_SUPPORT_FLASH
    bool "BSP_SUPPORT_FLASH"
    default y

config BSP_SUPPORT_TIMER
    bool "BSP_SUPPORT_TIMER"
    default y

config BSP_SUPPORT_WIFI
    bool "BSP_SUPPORT_WIFI"
    default y

endmenu

endif
