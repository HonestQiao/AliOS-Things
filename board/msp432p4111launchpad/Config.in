config AOS_BOARD_MSP432P4111LAUNCHPAD
    bool "msp432p4111launchpad"
    select AOS_MCU_MSP432P4XX
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_SAL if AOS_NETWORK_SAL
    select AOS_COMP_NETMGR if AOS_NETWORK_SAL
    help

if AOS_BOARD_MSP432P4111LAUNCHPAD
# Configurations for board msp432p4111launchpad

# Set default value of AOS_NETWORK_SAL
config AOS_NETWORK_SAL
    bool "AOS_NETWORK_SAL"
    default y

config SAL_MODULE
    string
    default "wifi.mk3060"

# "BSP SUPPORT FEATURE"
config BSP_SUPPORT_UART
    bool
    default y

config BSP_SUPPORT_FLASH
    bool
    default y

config BSP_SUPPORT_I2C
    bool
    default y

config BSP_SUPPORT_GPIO
    bool
    default y

config BSP_SUPPORT_WIFI
    bool
    default y

endif
