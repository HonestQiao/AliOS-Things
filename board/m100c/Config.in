config AOS_BOARD_M100C
    bool "board_m100c"
    select AOS_MCU_EFM32GXX
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_LORAWAN_4_4_2 if lorawanback = 0
    select AOS_COMP_LORAWAN_4_4_0 if lorawanback = 1
    help

if AOS_BOARD_M100C
# Configurations for board board_m100c

config lorawanback
    int "lorawanback"
    range 0 1
    default 0

menu "BSP SUPPORT FEATURE"

config BSP_SUPPORT_UART
    bool "BSP_SUPPORT_UART"
    default y

config BSP_SUPPORT_FLASH
    bool "BSP_SUPPORT_FLASH"
    default y

config BSP_SUPPORT_LORA
    bool "BSP_SUPPORT_LORA"
    default y

endmenu

endif
