config AOS_BOARD_NUMAKER-PFM-M487
    bool "numaker-pfm-m487"
    select AOS_MCU_M487JIDAE
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_NEWLIB_STUB
    select AOS_COMP_NETMGR
    select AOS_COMP_LWIP if ETHERNET_CONFIG = 1
    select AOS_COMP_YLOOP if ETHERNET_CONFIG = 1
    help

if AOS_BOARD_NUMAKER-PFM-M487
# Configurations for board numaker-pfm-m487

config ETHERNET_CONFIG
    int "ETHERNET"
    range 0 1
    default 1

endif
