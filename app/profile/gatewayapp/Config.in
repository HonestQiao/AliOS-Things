config AOS_APP_GATEWAYAPP
    bool "Gateway Profile"
    select AOS_COMP_SENSOR
    select AOS_COMP_CJSON
    select AOS_COMP_NETMGR
    select AOS_COMP_UDATA
    select AOS_COMP_MBMASTER
    select AOS_COMP_LINKKIT_SDK_C
    select APP_FEATURE_LINKKIT_NOAWSS if APP_GATEWAYAPP_CONFIG_DTC
    select AOS_COMP_LWIP if LWIP = 1 && APP_GATEWAYAPP_CONFIG_DTC
    select AOS_COMP_CLI if AOS_BOARD_ESP8266 && APP_GATEWAYAPP_CONFIG_DTC
#    select AOS_UNKNOWN_WEBSOCKETS if APP_GATEWAYAPP_CONFIG_GW_LINKED && APP_GATEWAYAPP_CONFIG_DT 
    help
      gateway profile

if AOS_APP_GATEWAYAPP
# Configurations for app gatewayapp

config LWIP
    int "LWIP"
    range 0 1
    default 0

config APP_GATEWAYAPP_CONFIG_PT_SCANNER
    int "Device model: scanner"
    range 0 1
    default 1
    help

config APP_GATEWAYAPP_CONFIG_PT_SENSOR
    int "Device model: MODBUS/CAN sensors"
    range 0 1
    default 0
    help

config AOS_CONFIG_DTC_ENABLE
    bool "data to cloud"
    default n
    help

endif
